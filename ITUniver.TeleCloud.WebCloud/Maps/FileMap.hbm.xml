<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    namespace="ITUniver.TeleCloud.WebCloud.Models" assembly="ITUniver.TeleCloud.WebCloud">

  <class name="FileItemModel" table="[TFile]">

    <id name="Id">
      <generator class="native" />
    </id>

    <property name="NameFile" />
    <property name="DateCreate" />
    
    <many-to-one name="IdUser" column="IdUser" class="Users"/>

  </class>

</hibernate-mapping>